{
  "name": "semantix-document-search-backend",
  "version": "1.0.0",
  "description": "Backend for Semantix Document Search",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "migrate": "ts-node src/db/migrate.ts",
    "migrate:reset": "psql -U $DB_USER -d $DB_NAME -f src/db/reset.sql && npm run migrate"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^0.0.42",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "aws-sdk": "^2.1692.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
